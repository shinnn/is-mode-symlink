{
  "name": "is-mode-symlink",
  "version": "1.0.0",
  "description": "Check if a file mode integer represents symbolic link",
  "repository": "shinnn/is-mode-symlink",
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/is-mode-symlink/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "file",
    "mode",
    "int",
    "integer",
    "number",
    "symbolic link",
    "link",
    "symlink",
    "check",
    "detect",
    "browser",
    "client-side"
  ],
  "devDependencies": {
    "eslint": "^0.14.1",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.11.3",
    "require-bower-files": "^2.0.0",
    "tap-spec": "^2.2.1",
    "tape": "^3.5.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
